<project name="BrokerDeploy" default="init">
<property name="barfile" location="/var/lib/jenkins/workspace/microservices/eureka/eureka_build/target" />
<property name="artifact.name" value="eureka-0.0.1-SNAPSHOT" />
<property name="artifact.version" value="1" />
<target name="init">
    	<antcall target="rename" />	
    </target>
    <target name="rename">
    	<move file="${barfile}/eureka-0.0.1-SNAPSHOT.jar" todir="${barfile}/${artifact.name}-${artifact.version}.jar"/>
	</target>
</project>